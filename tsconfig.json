{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "jsx": "react",

    "baseUrl": ".",
    "paths": {
      "components/*": ["src/components/*"],
      "screens/*": ["src/screens/*"],
      "hooks/*": ["src/hooks/*"],
      "types/*": ["src/types/*"],
      "utils/*": ["src/utils/*"],
      "redux/*": ["src/redux/*"],
      "src/*": ["src/*"]
    }
  }
}
